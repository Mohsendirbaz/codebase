/* Root-level global font settings */
html, body {
  font-family: 'Arial', sans-serif !important; /* Replace 'Arial' with your desired font */
  color: inherit; /* Keep color inheritance, no change to color */
  margin: 0;
  padding: 0;
  box-sizing: border-box;
}

/* Ensure that all elements inherit the global font settings */
*, *::before, *::after {
  font-family: inherit !important; /* Force inheritance of the font family */
  box-sizing: inherit;
}

.L_1_HomePage {
  display: grid;
  grid-template-columns: 1fr;
  grid-template-rows: auto auto auto;
  height: 100vh;
  width: 100vw;
  gap: 0px;
  background-color: #d6cbcb; /* Light background */
  position: relative;
  overflow: auto; /* Prevent scroll bars from appearing */
  justify-content: center;
  align-items: center;
}

.L_1_HomePageSectionA {
  background-color: #FFD700; /* Gold background */
  display: grid; /* Switch back to grid layout */
  grid-template-columns: 1fr; /* Only one column */
  grid-template-rows: 1fr; /* Only one row */
  position: relative;
  padding: 20px;
  color: #002366; /* Deep Blue text */
  font-weight: bold; /* Make the text bold */
  font-size: 1.2em; /* Increase the font size for a main title */
  text-shadow: 2px 2px 4px rgba(0, 0, 0, 0.3); /* Add a slight shadow */
}

/* Main Title Section in the Center */
.L_1_HomePageSectionT {
  grid-column: 1 / -1; /* Span across the entire grid */
  grid-row: 1 / -1;    /* Span across the entire grid */
  display: flex;
  justify-content: center; /* Center the content horizontally */
  align-items: center;     /* Center the content vertically */
  padding: 20px;
  color: #002366;
  font-weight: bold;
  font-size: 1em;
  text-shadow: 2px 2px 4px rgba(0, 0, 0, 0.3);
  flex-direction: column;
}

/* Image at Top Left */
.about-us-image1 {
  grid-column: 1 / -1; /* Span across the entire grid */
  grid-row: 1 / -1;    /* Span across the entire grid */
  justify-self: start; /* Align to the left */
  align-self: flex-start; /* Align to the top */
  max-width: 10%;
  max-height: 10%;
}

.about-us-image1 img {
  width: 100%;
  height: auto;
  object-fit: cover; /* Ensure it fits without distorting */
}

/* Essential Buttons at Bottom Left */
.EssentialButtons {
  grid-column: 1 / -1; /* Span across the entire grid */
  grid-row: 1 / -1;    /* Span across the entire grid */
  display: flex;
  flex-direction: row;
  justify-self: start; /* Align to the left */
  align-self: flex-end; /* Align to the bottom */
  gap: 10px; /* Space between buttons */
}

/* Theme Ribbon at Top Right */
.theme-ribbon {
  grid-column: 1 / -1; /* Span across the entire grid */
  grid-row: 1 / -1;    /* Span across the entire grid */
  display: flex;
  justify-self: end;   /* Align to the right */
  align-self: flex-start; /* Align to the top */
  margin-top: 20px;
  margin-right: 5%;    /* Optional: Adjust for slight right positioning */
}

.L_1_HomePageTabs {
  display: flex;
  align-items: flex-start;
  justify-content: left;
  background-color: #141818; /* Dark teal */
  position: sticky; /* Make the tab bar sticky */
  top: 0; /* Stick to the top of the container */
  z-index: 1000; /* Ensure it's above other elements */
  height: var(--tabs-height);
  grid-column: 1 / 3;
}


