/* Tooltip container */
.tooltip-container {
  position: relative;
  display: inline-block;
}

/* Tooltip text */
.tooltip1 {
  visibility: hidden;
  background-color: rgb(186, 240, 187); /* Green background color for hover */
  color: #000000;
  text-align: center;
  border-radius: 5px;
  padding: 5px 10px;
  position: absolute;
  z-index: 1;
  bottom: 120%; /* Position the tooltip above the button */
  left: 50%;
  transform: translateX(-50%); /* Center align the tooltip */
  width: 220px;
  font-size: 16px;
}

.tooltip1::after {
  content: "";
  position: absolute;
  top: 100%; /* Position the arrow at the bottom of the tooltip */
  left: 50%;
  transform: translateX(-50%);
  border-width: 10px; /* Larger arrow */
  border-style: solid;
  border-color: rgb(186, 240, 187) transparent transparent transparent; /* Arrow color matches green hover background */
}



/* Default tooltip, kept for compatibility */
.tooltip {
  visibility: hidden;
  background-color: rgb(186, 240, 187); /* Green background color for hover */
  color: #000000;
  text-align: center;
  border-radius: 5px;
  padding: 5px 10px;
  position: absolute;
  z-index: 1;
  bottom: 120%;
  left: 50%;
  transform: translateX(-50%);
  width: 220px;
  font-size: 16px;
}


.left-aligned-text {
  text-align: left;
}

/* Show tooltip1 on hover */
.increment-decrement1:hover .tooltip1,
.tooltip-container:hover .tooltip1 {
  visibility: visible;
  background-color: rgb(186, 240, 187); /* Green hover background */
}

/* No hover effect for default tooltip */
.tooltip-container:hover .tooltip {
  visibility: visible;
  background-color: rgb(186, 240, 187); /* Green hover background */
}



