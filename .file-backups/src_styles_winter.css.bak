/* Tooltip container */
.tooltip2 {
  visibility: hidden;
  background-color: black;
  color: white;
  text-align: center;
  padding: 5px;
  border-radius: 4px;
  position: absolute;
  z-index: 1;
  bottom: 125%; /* Position above the header */
  left: 50%;
  transform: translateX(-50%);
  opacity: 0;
  transition: opacity 0.3s;
}

/* Tooltip arrow */
.tooltip2::after {
  content: "";
  position: absolute;
  top: 100%; /* At the bottom of the tooltip */
  left: 50%;
  margin-left: -5px;
  border-width: 5px;
  border-style: solid;
  border-color: black transparent transparent transparent;
}

/* Show tooltip on hover */
.custom-header:hover .tooltip2 {
  visibility: visible;
  opacity: 1;
}


.gear {
  width: 66px;
  height: 66px;
  background-color:  #4682b4; /* Deep Sky Blue on hover */
  clip-path: polygon(
    30% 0%, 70% 0%,  /* Top */
    100% 30%,        /* Top-right */
    100% 70%,        /* Bottom-right */
    70% 100%, 30% 100%, /* Bottom */
    0% 70%,          /* Bottom-left */
    0% 30%           /* Top-left */
  );
  display: flex;
  justify-content: center;
  align-items: center;
  transition: transform 0.3s ease;
  cursor: pointer;
  position: relative;
}

.gear-segment {
  color: white;
  position: absolute;
  display: flex;
  justify-content: space-around;
  align-items: center;
}

.gear-segment:hover {
  background-color: #4682b4; /* Deep Sky Blue on hover */
}

.gear-segment.top { top: -20px; left: 50px; }
.gear-segment.top-right { top: 10px; right: -20px; transform: rotate(45deg); }
.gear-segment.right { top: 50px; right: -20px; }
.gear-segment.bottom-right { bottom: 10px; right: -20px; transform: rotate(-45deg); }
.gear-segment.bottom { bottom: -20px; left: 50px; }
.gear-segment.bottom-left { bottom: 10px; left: -20px; transform: rotate(45deg); }
.gear-segment.left { top: 50px; left: -20px; }
.gear-segment.top-left { top: 10px; left: -20px; transform: rotate(-45deg); }

.gear-center {
  text-align: center;
  font-size: 2em;
  font-weight: bold;
  color: white;
}
